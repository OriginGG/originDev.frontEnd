{
  "name": "new_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@loadable/component": "^5.7.0",
    "antd": "^3.4.0",
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-client": "^2.2.5",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "0.28.7",
    "dayjs": "^1.8.8",
    "default-passive-events": "^1.0.10",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "draftjs-to-html": "^0.8.4",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "faker": "^4.1.0",
    "file-loader": "1.1.5",
    "formik": "^0.11.11",
    "fs-extra": "3.0.1",
    "googleapis": "^36.0.0",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "history": "^4.7.2",
    "html-to-react": "^1.3.3",
    "html-to-react-components": "^1.6.3",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.2.0",
    "logrocket": "^0.6.21",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "pouchdb": "^6.4.3",
    "primereact": "^1.5.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.8.6",
    "react-alice-carousel": "^1.10.4",
    "react-apollo": "^2.0.4",
    "react-burger-menu": "^2.4.2",
    "react-datetime": "^2.16.2",
    "react-dev-utils": "^5.0.0",
    "react-device-detect": "^1.1.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.3",
    "react-draft-wysiwyg": "^1.13.2",
    "react-driftjs": "^1.1.6",
    "react-dropzone": "^4.2.9",
    "react-favicon": "^0.0.14",
    "react-fullstory": "^1.0.3",
    "react-ga": "^2.5.2",
    "react-gtm-module": "^2.0.2",
    "react-html-parser": "^2.0.2",
    "react-jss": "^8.3.3",
    "react-loader-spinner": "^2.3.0",
    "react-on-images-loaded": "^2.0.7",
    "react-responsive-modal": "^2.0.1",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.1",
    "react-smooth-dnd": "^0.11.0",
    "react-spinkit": "^3.0.0",
    "react-stripe-elements": "^2.0.1",
    "react-svg-spinner": "^1.0.1",
    "react-swipe": "^6.0.4",
    "react-toastify": "^3.4.3",
    "react-twitter-widgets": "^1.7.1",
    "reacterminator": "^0.15.1",
    "rsuite": "^3.8.4",
    "semantic-ui-react": "^0.79.0",
    "slick-carousel": "^1.8.1",
    "store": "^2.0.12",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "swipe-js-iso": "^2.1.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "dotenv -e .env.local node scripts/start.js",
    "build": "node scripts/build.js",
    "build-dev": "dotenv -e .env.development node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd"
        }
      ],
      [
        "transform-semantic-ui-react-imports",
        {
          "convertMemberImports": true,
          "importType": "es",
          "addCssImports": false,
          "importMinifiedCssFiles": false,
          "addLessImports": false,
          "addDuplicateStyleImports": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/label-has-for": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "import/no-named-as-default": "off",
      "arrow-parens": [
        "off"
      ],
      "compat/compat": "off",
      "consistent-return": "off",
      "comma-dangle": "off",
      "camelcase": "off",
      "no-debugger": "warn",
      "max-len": "off",
      "no-lonely-if": "off",
      "guard-for-in": "off",
      "no-restricted-syntax": "off",
      "function-paren-newline": "off",
      "flowtype-errors/show-errors": "off",
      "generator-star-spacing": "off",
      "import/prefer-default-export": "off",
      "import/extensions": "off",
      "react/no-unknown-property": "off",
      "import/no-unresolved": "off",
      "no-restricted-globals": "off",
      "import/no-extraneous-dependencies": "off",
      "promise/catch-or-return": "off",
      "object-curly-newline": "off",
      "no-console": "off",
      "arrow-body-style": "off",
      "global-require": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "no-await-in-loop": "off",
      "no-tabs": "off",
      "no-underscore-dangle": "off",
      "indent": "off",
      "no-bitwise": "off",
      "no-use-before-define": "off",
      "no-unneeded-ternary": "off",
      "linebreak-style": "off",
      "no-multi-assign": "off",
      "no-multi-spaces": "off",
      "promise/param-names": "error",
      "promise/always-return": "off",
      "promise/no-native": "off",
      "react/no-array-index-key": "off",
      "react/sort-comp": [
        "error",
        {
          "order": [
            "type-annotations",
            "static-methods",
            "lifecycle",
            "everything-else",
            "render"
          ]
        }
      ],
      "react/jsx-closing-bracket-location": "off",
      "react/jsx-no-bind": "off",
      "react/jsx-indent": "off",
      "react/no-danger": "off",
      "react/jsx-wrap-multilines": "off",
      "react/no-multi-comp": "off",
      "react/forbid-prop-types": "off",
      "react/no-did-mount-set-state": "off",
      "react/jsx-closing-tag-location": "off",
      "react/jsx-indent-props": "off",
      "react/jsx-boolean-value": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prefer-stateless-function": "off"
    },
    "plugins": [
      "import",
      "promise",
      "compat",
      "graphql",
      "react"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-semantic-ui-react-imports": "^1.4.1",
    "compression-webpack-plugin": "^1.1.7",
    "dotenv-cli": "^1.4.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-graphql": "^1.5.0",
    "eslint-plugin-promise": "^3.6.0",
    "prettydiff": "^99.0.1"
  }
}
